	.data-aqi
		display flex

		div
			border-right: 1px solid #ddd;
			width: calc(100%/2);
			min-width: 100px;
			line-height: 2.58;

		div:nth-child(2)
			border-right: 0px
		
	.data-avg-min-max
		display flex

		div 
			border-right 1px solid #dddddd
			width calc(100%/3)
			min-width 100px
			line-height 2.58

		div:nth-child(3n)
			border none
