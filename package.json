{
  "name": "kolkata_flood_monitoring_webapp",
  "version": "1.2.0",
  "description": "A portal to monitor flood in kolkata ",
  "main": "index.js",
  "scripts": {
    "flow": "flow check",
    "flow:deps": "flow-typed install",
    "deploy_at": "aurassure",
    "backend_port": "46113",
    "generate_sri_for_js_file": "gulp generate_hashes_for_js_file && gulp generate_sri_for_js_file",
    "generate_build_files_to_deploy": "npm run generate_sri_for_js_file && cross-env BUILD_TYPE=deploy npm run build_php_file",
    "generate_build_files_to_stage": "npm run generate_sri_for_js_file && cross-env BUILD_TYPE=stage npm run build_php_file",
    "build_php_file": "gulp php_file",
    "stage:frontend": "gulp react_hash_router && cross-env NODE_ENV=production BUILD_TYPE=stage gulp frontend && npm run generate_build_files_to_stage && gulp upload_frontend_files_to_testing_server",
    "stage:backend": "cross-env NODE_ENV=production BUILD_TYPE=stage gulp backend && gulp upload_backend_files_to_testing_server",
    "stage": "npm run stage:frontend && npm run stage:backend",
    "upload_frontend_files_to_deploy_servers": "gulp upload_js_files_to_deploy_servers && gulp upload_php_files_to_deploy_servers",
    "upload_files_to_testing_server": "gulp upload_files_to_testing_server",
    "deploy_build:frontend": "gulp react_browser_router && cross-env NODE_ENV=production BUILD_TYPE=deploy gulp frontend && npm run generate_build_files_to_deploy && gulp react_hash_router",
    "deploy_build:backend": "cross-env NODE_ENV=production BUILD_TYPE=deploy gulp backend",
    "deploy_build": "npm run deploy_build:frontend && npm run deploy_build:backend",
    "deploy:frontend": "npm run deploy_build:frontend && npm run upload_frontend_files_to_deploy_servers",
    "deploy:backend": "npm run deploy_build:backend && gulp upload_backend_files_to_deploy_servers",
    "deploy": "npm i && npm run deploy:frontend && npm run deploy:backend",
    "doc:api": "apidoc -i src/api/ -o DOCUMENTATION/Api_Documentation/ -f .js",
    "doc:app": "esdoc -c esdoc.json",
    "test": "npm run flow && jest --coverage ./test_script",
    "test:frontend": "jest --watch ./test_script/app",
    "test:backend": "npm run resetdb && npm run pushdummydata && cross-env jest ./test_scripts/api/api.test.js",
    "start:frontend": "cross-env NODE_ENV=development gulp frontend",
    "start:backend": "gulp backend",
    "resetdb": "node demo_db_setup/create_demo_db.js",
    "pushdummydata": "node demo_db_setup/dummy_data.js"
  },
